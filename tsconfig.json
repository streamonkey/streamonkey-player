{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "strict": true,
        "declaration": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "incremental": true,
        "isolatedModules": true,
        "removeComments": true,
        "newLine": "lf",
        "pretty": true,
        "tsBuildInfoFile": ".tsbuildinfo"
    },
    "exclude": [
        "browserwrapper.ts",
        "node_modules",
        "browser",
        "dist",
        "test.ts"
    ]
}